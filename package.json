{
  "name": "vpn-api",
  "version": "1.0.0",
  "description": "vpn-api",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:dev": "node -r ts-node/register -r dotenv/config src/index.ts",
    "start:w": "nodemon --watch src --ext ts,json --exec 'pnpm start:dev'",
    "clear": "rm -rf dist",
    "test:unit": "jest --config jest.config.json",
    "test:e2e": "jest --runInBand --config jest.config.e2e.json",
    "test:w": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "eslint --quiet --ext ts,js .",
    "lint:fix": "eslint --fix --ext .ts .",
    "core": "pnpm add core@git+git@github.com:layereddd/core.git#main"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pshenok/vpn-api"
  },
  "author": "pshenok",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@prisma/client": "3.15.2",
    "@types/uuid": "^8.3.4",
    "awilix": "^6.1.0",
    "axios": "^0.27.2",
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.0.1",
    "core": "git@github.com:layereddd/core.git#main",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "fast-safe-stringify": "^2.1.1",
    "helmet": "^5.0.2",
    "prisma": "^3.15.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.3",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.11",
    "@types/glob": "^7.1.1",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.0.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.3.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
